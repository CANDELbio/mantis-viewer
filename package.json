{
  "name": "mantis-viewer",
  "version": "0.7.1",
  "description": "Electron application to visualize multidimensional microscopy data",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "tsc main.ts --sourceMap && webpack",
    "pack": "build --dir",
    "dist": "build -mwl --x64"
  },
  "repository": "https://github.com/ParkerICI/imc-viewer-js",
  "keywords": [
    "Electron"
  ],
  "build": {
    "appId": "com.mantis-viewer.app",
    "mac": {
      "category": "public.app-category.medical",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": false
    },
    "linux": {
      "target": "AppImage",
      "category": "Development"
    }
  },
  "author": "pfgherardini",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/concaveman": "^1.1.3",
    "@types/csv-parse": "^1.1.11",
    "@types/d3-scale": "^2.0.1",
    "@types/node": "^8.0.0",
    "@types/pixi.js": "^4.8.0",
    "@types/plotly.js": "^1.38.8",
    "@types/react": "15.6.19",
    "@types/react-collapse": "^4.0.2",
    "@types/react-color": "^2.13.6",
    "@types/react-dom": "^15.5.8",
    "@types/react-motion": "0.0.27",
    "@types/react-select": "^1.3.4",
    "@types/shortid": "0.0.29",
    "@types/underscore": "^1.8.0",
    "css-loader": "^0.27.3",
    "electron": "^3.0.2",
    "electron-builder": "^20.28.4",
    "ify-loader": "^1.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.16.0",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.1",
    "webpack": "~4.21.0",
    "webpack-cli": "^3.1.2",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.6.1",
    "bootstrap": "^4.1.3",
    "concaveman": "^1.1.1",
    "csv-parse": "^3.1.3",
    "d3-scale": "^2.1.2",
    "flexbox-react": "^4.4.0",
    "jquery": "^3.3.1",
    "mobx": "^5.1.2",
    "mobx-react": "^5.2.8",
    "mobx-utils": "^5.0.2",
    "normalize.css": "^5.0.0",
    "pixi.js": "^4.8.2",
    "plotly.js": "^1.41.1",
    "react": "^15.6.2",
    "react-collapse": "^4.0.3",
    "react-color": "^2.14.1",
    "react-dom": "^15.6.2",
    "react-motion": "^0.5.2",
    "react-select": "^1.3.0",
    "react-spinners": "^0.4.5",
    "react-transition-group": "^2.5.0",
    "shortid": "^2.2.13",
    "tiff": "^3.0.0",
    "underscore": "^1.8.3"
  }
}
