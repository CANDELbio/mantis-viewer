{
  "name": "imc-viewer",
  "version": "0.5.2",
  "description": "Electron application to visualize IMC data",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "tsc main.ts renderer.ts --sourceMap && webpack",
    "pack": "build --dir",
    "dist": "build -mwl --x64"
  },
  "repository": "https://github.com/ParkerICI/imc-viewer-js",
  "keywords": [
    "Electron"
  ],
  "build": {
    "appId": "com.imc-viewer.app",
    "mac": {
      "category": "public.app-category.medical",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": false
    },
    "linux": {
      "target": "AppImage",
      "category": "Development"
    }
  },
  "author": "pfgherardini",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/d3-scale": "^2.0.1",
    "@types/node": "^8.0.0",
    "@types/pixi.js": "^4.7.5",
    "@types/plotly.js": "^1.38.6",
    "@types/react": "15.6.19",
    "@types/react-dom": "^15.5.8",
    "@types/react-select": "^1.3.4",
    "@types/shortid": "0.0.29",
    "@types/underscore": "^1.8.0",
    "css-loader": "^0.27.3",
    "electron": "^1.7.10",
    "electron-builder": "^20.28.1",
    "ify-loader": "^1.1.0",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.16.0",
    "ts-loader": "^4.5.0",
    "typescript": "^2.9.2",
    "webpack": "~4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.11.1",
    "@blueprintjs/table": "^1.19.0",
    "d3-scale": "^2.1.2",
    "mobx": "^5.1.2",
    "mobx-react": "^5.2.8",
    "mobx-utils": "^5.0.2",
    "normalize.css": "^5.0.0",
    "pixi.js": "^4.8.1",
    "plotly.js": "^1.40.1",
    "react": "^15.6.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-flexbox-grid": "^2.1.2",
    "react-select": "^1.3.0",
    "shortid": "^2.2.13",
    "tiff": "^3.0.0",
    "underscore": "^1.8.3"
  }
}
