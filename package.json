{
    "name": "mantis-viewer",
    "version": "1.2.2-beta.2",
    "description": "Electron application to visualize multidimensional microscopy data",
    "main": "app/main.js",
    "scripts": {
        "start": "webpack --watch --config webpack.dev.js",
        "build": "webpack --config webpack.prod.js",
        "electron": "electron . debug",
        "pack": "webpack --config webpack.prod.js && electron-builder --dir",
        "dist-mac": "webpack --config webpack.prod.js && electron-builder -ml --x64",
        "dist-linux": "webpack --config webpack.prod.js && electron-builder -l --x64",
        "dist-win": "webpack --config webpack.prod.js && electron-builder -w --x64",
        "publish-mac": "webpack --config webpack.prod.js && electron-builder -m --x64 -p onTag",
        "publish-linux": "webpack --config webpack.prod.js && electron-builder -l --x64 -p onTag",
        "publish-win": "webpack --config webpack.prod.js && electron-builder -w --x64 -p onTag",
        "test": "jest",
        "eslint": "eslint --ext .tsx --ext .ts .",
        "eslint-fix": "eslint --fix --ext .tsx --ext .ts .",
        "postinstall": "husky install && electron-rebuild",
        "prepare": "husky install",
        "postversion": "git push && git push --tags"
    },
    "repository": "https://github.com/ParkerICI/mantis-viewer",
    "keywords": [
        "Electron",
        "Microscopy"
    ],
    "build": {
        "appId": "com.mantis-viewer.app",
        "npmRebuild": false,
        "mac": {
            "category": "public.app-category.medical",
            "target": "dmg",
            "publish": [
                "github"
            ]
        },
        "win": {
            "target": "nsis",
            "publish": [
                "github"
            ]
        },
        "nsis": {
            "oneClick": false,
            "allowElevation": false
        },
        "linux": {
            "target": [
                {
                    "target": "AppImage"
                },
                {
                    "target": "deb"
                }
            ],
            "publish": [
                "github"
            ],
            "maintainer": "engineering@parkerici.org",
            "category": "Development"
        }
    },
    "author": "pici",
    "license": "GPL-3.0",
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@types/better-sqlite3": "^5.4.1",
        "@types/concaveman": "^1.1.3",
        "@types/d3-scale": "^2.0.1",
        "@types/electron-devtools-installer": "^2.2.1",
        "@types/jest": "^24.0.9",
        "@types/node": "^18.11.9",
        "@types/plotly.js": "~1.50.16",
        "@types/react": "16.8.4",
        "@types/react-color": "^2.13.6",
        "@types/react-dom": "^16.8.2",
        "@types/react-motion": "0.0.27",
        "@types/react-select": "^3.0.14",
        "@types/shortid": "0.0.29",
        "@types/underscore": "^1.9.2",
        "@types/xml2js": "^0.4.5",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.27.0",
        "acorn": "^6.4.1",
        "bubleify": "^2.0.0",
        "buffer": "^6.0.3",
        "canvas": "^2.9.0",
        "css-loader": "^6.7.2",
        "electron": "19.1.5",
        "electron-builder": "^23.0.8",
        "electron-connect": "^0.6.3",
        "electron-rebuild": "^3.2.7",
        "electron-reload-webpack-plugin": "^2.0.4",
        "electron-updater": "^5.3.0",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "file-loader": "^6.2.0",
        "husky": "^7.0.4",
        "ify-loader": "^1.1.0",
        "jest": "^24.3.0",
        "lint-staged": "^12.3.3",
        "node-abi": "^3.28.0",
        "prettier": "^2.5.1",
        "raw-loader": "^4.0.2",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.16.0",
        "ts-jest": "^24.0.0",
        "ts-loader": "^7.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.5.5",
        "webpack": "~5.68.0",
        "webpack-cli": "^4.9.2",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@blueprintjs/core": "^3.40.1",
        "@types/tinycolor2": "^1.4.3",
        "about-window": "^1.15.2",
        "better-sqlite3": "^7.6.2",
        "bindings": "^1.5.0",
        "bootstrap": "^4.3.1",
        "concaveman": "^1.1.1",
        "csv-parse": "^4.8.8",
        "csv-stringify": "^5.3.6",
        "d3-scale": "^2.1.2",
        "electron-context-menu": "^3.1.1",
        "electron-is-dev": "^2.0.0",
        "electron-log": "^4.4.6",
        "electron-store": "^8.0.1",
        "geotiff": "2.0.4",
        "hotkeys-js": "^3.8.7",
        "jquery": "^3.5.0",
        "mobx": "^5.15.7",
        "mobx-react": "^5.4.4",
        "mobx-utils": "^5.6.1",
        "normalize.css": "^5.0.0",
        "npm": "^8.8.0",
        "pixi.js": "^6.3.0",
        "plotly.js": "~1.58.4",
        "pretty-ms": "^5.0.0",
        "react": "^16.8.2",
        "react-color": "^2.17.3",
        "react-contextmenu": "^2.14.0",
        "react-dom": "^16.8.2",
        "react-flexbox-grid": "^2.1.2",
        "react-icons": "^3.7.0",
        "react-motion": "^0.5.2",
        "react-numeric-input2": "^3.1.0",
        "react-select": "^3.1.0",
        "react-sizeme": "^2.6.7",
        "react-table-container": "^2.0.3",
        "reactstrap": "^8.10.1",
        "shortid": "^2.2.16",
        "tinycolor2": "^1.4.2",
        "underscore": "^1.12.1",
        "xml2js": "^0.4.23"
    },
    "optionalDependencies": {
        "fsevents": "^2.1.2"
    }
}
